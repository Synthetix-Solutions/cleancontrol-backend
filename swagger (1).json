  "components": {
    "schemas": {
      "CleaningRun": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "cleaningTeam": {
            "$ref": "#/components/schemas/CleaningTeam"
          },
          "startingRoom": {
            "$ref": "#/components/schemas/Room"
          }
        },
        "additionalProperties": false
      },
      "CleaningTask": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "recurrenceInterval": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "onCheckout": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "CleaningTeam": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "cleaners": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/User"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "InventoryItem": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Role": {
        "enum": [
          0,
          1,
          2,
          3
        ],
        "type": "integer",
        "format": "int32"
      },
      "Room": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "roomNumber": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RoomRefill": {
        "type": "object",
        "properties": {
          "item": {
            "$ref": "#/components/schemas/InventoryItem"
          },
          "room": {
            "$ref": "#/components/schemas/Room"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "refillTime": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "A unique identifier for the User.",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "description": "The name of the User.",
            "nullable": true
          },
          "username": {
            "type": "string",
            "description": "The username of the User.",
            "nullable": true
          },
          "role": {
            "$ref": "#/components/schemas/Role"
          }
        },
        "additionalProperties": false,
        "description": "Represents a User in the system."
      }
    }
  }
